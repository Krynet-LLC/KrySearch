{
  "site": {
    "name": "KrySearch",
    "description": "Privacy-first search router for open and privacy-focused engines",
    "url": "https://krysearch.io",
    "logo": "https://cdn.discordapp.com/attachments/1458158342739595265/1458204648404947018/file_00000000ae4471fda1a4f9cc24f64be5.png?ex=695eca68&is=695d78e8&hm=9065c921c86ec91e16a0c69e0c3df4cf47f47cda1cd8b35cb929c92dd57ad801&animated=true",
    "theme": "dark",
    "language": "en-US"
  },
  "search": {
    "defaultEngine": "startpage",
    "routing": "engine-query",
    "noInputParsing": true,
    "maxResults": 50,
    "safeSearch": true,
    "autocomplete": true,
    "highlightTerms": true
  },
  "engines": {
    "open_source": {
      "searxng": {
        "name": "SearXNG",
        "description": "Open-source metasearch engine aggregating results privately",
        "base": "https://searx.space/",
        "queryParam": "q",
        "directParam": "url",
        "mode": "direct"
      },
      "yacy": {
        "name": "YaCy",
        "description": "Decentralized, peer-to-peer search engine",
        "base": "http://search.yacy.net/",
        "queryParam": "query",
        "directParam": "url",
        "mode": "direct"
      },
      "mojeek": {
        "name": "Mojeek",
        "description": "Independent crawler and index, no tracking",
        "base": "https://www.mojeek.com/search",
        "queryParam": "q",
        "directParam": "url",
        "mode": "direct"
      },
      "metager": {
        "name": "MetaGer",
        "description": "Open-source privacy-oriented metasearch from Germany",
        "base": "https://metager.org/search",
        "queryParam": "q",
        "directParam": "url",
        "mode": "direct"
      }
    },
    "closed_source": {
      "startpage": {
        "name": "Startpage",
        "description": "Google results without tracking",
        "base": "https://www.startpage.com/sp/search",
        "queryParam": "query",
        "directParam": "url",
        "mode": "direct"
      },
      "duckduckgo": {
        "name": "DuckDuckGo",
        "description": "Privacy-focused search engine",
        "base": "https://duckduckgo.com/",
        "queryParam": "q",
        "directParam": "url",
        "mode": "direct"
      },
      "brave": {
        "name": "Brave Search",
        "description": "Independent privacy-focused search by Brave",
        "base": "https://search.brave.com/search",
        "queryParam": "q",
        "directParam": "url",
        "mode": "direct"
      },
      "swisscows": {
        "name": "Swisscows",
        "description": "Family-friendly privacy-first search",
        "base": "https://swisscows.com/web",
        "queryParam": "query",
        "directParam": "url",
        "mode": "direct"
      },
      "presearch": {
        "name": "Presearch",
        "description": "Decentralized privacy search engine",
        "base": "https://presearch.com/search",
        "queryParam": "q",
        "directParam": "url",
        "mode": "direct"
      }
    }
  },
  "appearance": {
    "font": "Inter, sans-serif",
    "colors": {
      "primary": "#4b8df8",
      "secondary": "#1f1f1f",
      "accent": "#f8c848"
    },
    "customCSS": "UI/styles.css",
    "enableDarkModeSwitch": true
  }
}
